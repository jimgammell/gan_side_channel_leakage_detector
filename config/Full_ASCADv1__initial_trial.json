{
    "dataset_constructor": "ASCADv1",
    "save_dir": "full_ascad__initial_trial",
    "num_training_steps": 200000,
    "num_val_measurements": 100,
    "dataset_kwargs": {
        "use_full_traces": true,
        "full_trace_interval": [0, 100000],
        "target_bytes": 2,
        "store_in_ram": false
    },
    "dataloader_kwargs": {
        "batch_size": 64,
        "num_workers": 4,
        "pin_memory": true
    },
    "classifier_constructor": "ProuffNet",
    "classifier_kwargs": {
        "pool_size": 4,
        "conv_stride": 2
    },
    "classifier_optimizer_kwargs": {"lr": 1e-4},
    "mask_optimizer_kwargs": {"lr": 1e-4},
    "classifier_use_sam": true,
    "snr_targets": [
        "r_out",
        {"target_variable": "masked_subbytes", "target_byte": 2}
    ],
    "sweep_vals": {
        "mask_l1_decay": [1e1]
    },
    "mask_es_metric": "cosine_sim",
    "maximize_mask_es_metric": true
}